{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "camelcase": "off",
        "prefer-destructuring": "warn",
        "no-console": "off",
        "arrow-body-style": "warn",
        "semi": "warn",
        "no-unused-vars": "warn",
        "import/extensions": "off"
    }
}